{
  "extends": ["standard-react", "semistandard", "prettier"],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "max-len": ["error", {"code": 80, "ignoreUrls": true}],
    "quotes": ["error", "single"]
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "node": true
  }
}
